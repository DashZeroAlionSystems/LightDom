{
  "name": "@lightdom/seo-sdk",
  "version": "1.0.0",
  "description": "LightDom SEO SDK - Injectable JavaScript for automated SEO optimization",
  "main": "../../dist/sdk/lightdom-seo.js",
  "module": "../../dist/sdk/lightdom-seo.esm.js",
