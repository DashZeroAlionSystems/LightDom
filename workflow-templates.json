{
  "templates": [
    {
      "id": "seo-analysis-workflow",
      "name": "SEO Analysis Workflow",
      "description": "Automated SEO analysis and optimization workflow",
      "category": "seo",
      "tags": ["seo", "analysis", "automation"],
      "nodes": [
        {
          "id": "trigger-1",
          "type": "trigger",
          "position": { "x": 100, "y": 100 },
          "data": {
            "label": "Daily Trigger",
            "type": "trigger",
            "config": {
              "schedule": "0 0 * * *",
              "timezone": "UTC"
            }
          }
        },
        {
          "id": "http-1",
          "type": "httpRequest",
          "position": { "x": 300, "y": 100 },
          "data": {
            "label": "Fetch Website",
            "type": "httpRequest",
            "config": {
              "method": "GET",
              "url": "{{target_url}}",
              "timeout": 30000
            }
          }
        },
        {
          "id": "seo-1",
          "type": "seoAnalysis",
          "position": { "x": 500, "y": 100 },
          "data": {
            "label": "Analyze SEO",
            "type": "seoAnalysis",
            "config": {
              "checks": ["meta", "headings", "links", "images", "performance"]
            }
          }
        },
        {
          "id": "ai-1",
          "type": "aiProcess",
          "position": { "x": 700, "y": 100 },
          "data": {
            "label": "Generate Recommendations",
            "type": "aiProcess",
            "config": {
              "model": "gpt-4",
              "prompt": "Analyze SEO data and provide optimization recommendations"
            }
          }
        },
        {
          "id": "db-1",
          "type": "database",
          "position": { "x": 900, "y": 100 },
          "data": {
            "label": "Store Results",
            "type": "database",
            "config": {
              "operation": "insert",
              "table": "seo_analysis_results"
            }
          }
        },
        {
          "id": "notify-1",
          "type": "notification",
          "position": { "x": 1100, "y": 100 },
          "data": {
            "label": "Send Notification",
            "type": "notification",
            "config": {
              "method": "email",
              "to": "{{admin_email}}",
              "subject": "SEO Analysis Complete"
            }
          }
        }
      ],
      "edges": [
        { "id": "e1", "source": "trigger-1", "target": "http-1" },
        { "id": "e2", "source": "http-1", "target": "seo-1" },
        { "id": "e3", "source": "seo-1", "target": "ai-1" },
        { "id": "e4", "source": "ai-1", "target": "db-1" },
        { "id": "e5", "source": "db-1", "target": "notify-1" }
      ],
      "variables": [
        {
          "name": "target_url",
          "description": "URL to analyze",
          "required": true,
          "default": "https://example.com"
        },
        {
          "name": "admin_email",
          "description": "Email for notifications",
          "required": true,
          "default": "admin@example.com"
        }
      ]
    },
    {
      "id": "data-mining-workflow",
      "name": "Data Mining Workflow",
      "description": "Extract and analyze data from web sources",
      "category": "data-mining",
      "tags": ["data", "mining", "crawler"],
      "nodes": [
        {
          "id": "webhook-1",
          "type": "webhook",
          "position": { "x": 100, "y": 100 },
          "data": {
            "label": "Webhook Trigger",
            "type": "webhook",
            "config": {
              "path": "/mine-data",
              "method": "POST"
            }
          }
        },
        {
          "id": "mining-1",
          "type": "dataMining",
          "position": { "x": 300, "y": 100 },
          "data": {
            "label": "Mine Data",
            "type": "dataMining",
            "config": {
              "source": "{{source_url}}",
              "selectors": {
                "title": "h1",
                "content": ".content",
                "links": "a[href]"
              }
            }
          }
        },
        {
          "id": "function-1",
          "type": "function",
          "position": { "x": 500, "y": 100 },
          "data": {
            "label": "Transform Data",
            "type": "function",
            "config": {
              "code": "return items.map(item => ({ ...item, processed: true }));"
            }
          }
        },
        {
          "id": "db-1",
          "type": "database",
          "position": { "x": 700, "y": 100 },
          "data": {
            "label": "Store Data",
            "type": "database",
            "config": {
              "operation": "insert",
              "table": "mined_data"
            }
          }
        }
      ],
      "edges": [
        { "id": "e1", "source": "webhook-1", "target": "mining-1" },
        { "id": "e2", "source": "mining-1", "target": "function-1" },
        { "id": "e3", "source": "function-1", "target": "db-1" }
      ],
      "variables": [
        {
          "name": "source_url",
          "description": "URL to mine data from",
          "required": true,
          "default": "https://example.com"
        }
      ]
    },
    {
      "id": "ai-content-pipeline",
      "name": "AI Content Generation Pipeline",
      "description": "Generate and process content using AI",
      "category": "ai",
      "tags": ["ai", "content", "automation"],
      "nodes": [
        {
          "id": "trigger-1",
          "type": "trigger",
          "position": { "x": 100, "y": 100 },
          "data": {
            "label": "Hourly Trigger",
            "type": "trigger",
            "config": {
              "schedule": "0 * * * *"
            }
          }
        },
        {
          "id": "ai-1",
          "type": "aiProcess",
          "position": { "x": 300, "y": 100 },
          "data": {
            "label": "Generate Content",
            "type": "aiProcess",
            "config": {
              "model": "gpt-4",
              "prompt": "{{content_prompt}}",
              "temperature": 0.7
            }
          }
        },
        {
          "id": "decision-1",
          "type": "decision",
          "position": { "x": 500, "y": 100 },
          "data": {
            "label": "Quality Check",
            "type": "decision",
            "config": {
              "condition": "{{quality_score}} > 0.8"
            }
          }
        },
        {
          "id": "db-1",
          "type": "database",
          "position": { "x": 700, "y": 50 },
          "data": {
            "label": "Store Approved",
            "type": "database",
            "config": {
              "operation": "insert",
              "table": "approved_content"
            }
          }
        },
        {
          "id": "notify-1",
          "type": "notification",
          "position": { "x": 700, "y": 150 },
          "data": {
            "label": "Review Required",
            "type": "notification",
            "config": {
              "method": "email",
              "subject": "Content needs review"
            }
          }
        }
      ],
      "edges": [
        { "id": "e1", "source": "trigger-1", "target": "ai-1" },
        { "id": "e2", "source": "ai-1", "target": "decision-1" },
        { "id": "e3", "source": "decision-1", "target": "db-1", "label": "Approved" },
        { "id": "e4", "source": "decision-1", "target": "notify-1", "label": "Rejected" }
      ],
      "variables": [
        {
          "name": "content_prompt",
          "description": "Prompt for AI content generation",
          "required": true,
          "default": "Write a blog post about..."
        }
      ]
    }
  ]
}
