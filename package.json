{
  "name": "lightdom",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node start-complete-system.js",
    "start:framework": "node dist/framework/start.js",
    "start:blockchain": "node start-blockchain.js",
    "start:dev": "tsx src/framework/start.ts",
    "dev": "vite",
    "headless": "tsx src/apps/HeadlessCLI.ts",
    "headless:demo": "tsx src/apps/HeadlessDemo.ts",
    "headless:server": "tsx src/server/HeadlessAPIServer.ts",
    "api": "node api-server-express.js",
    "blockchain": "node start-blockchain.js",
    "automation": "node start-blockchain-automation.js",
    "automation:start": "node start-blockchain-automation.js",
    "automation:dev": "node start-blockchain-simple.js",
    "automation:debug": "node start-blockchain-automation.js --debug",
    "automation:dry-run": "node start-blockchain-automation.js --dry-run",
    "build": "tsc && vite build",
    "build:framework": "tsc src/framework/*.ts --outDir dist/framework --target es2020 --module commonjs --moduleResolution node",
    "demo:storage": "tsx src/framework/storage-examples.ts",
    "demo:automation": "tsx src/framework/automation-demo.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:contracts": "cd contracts && forge test",
    "test:api": "mocha test/api/**/*.test.js --timeout 30000",
    "test:frontend": "vitest run test/frontend/**/*.test.jsx",
    "test:crawler": "mocha test/crawler/**/*.test.js --timeout 30000",
    "test:blockchain": "mocha test/blockchain/**/*.test.js --timeout 30000",
    "test:browserbase": "mocha test/browserbase/**/*.test.js --timeout 60000",
    "test:integration": "mocha test/e2e/**/*.test.js --timeout 60000",
    "test:performance": "mocha test/performance/**/*.test.js --timeout 120000",
    "test:all": "npm run test:contracts && npm run test:api && npm run test:frontend && npm run test:crawler && npm run test:blockchain && npm run test:browserbase && npm run test:integration",
    "test:unit": "jest --testPathPattern=unit",
    "test:unit:coverage": "jest --testPathPattern=unit --coverage",
    "test:e2e": "playwright test",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:load": "artillery run tests/load/load-test.yml",
    "test:coverage:check": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "security:scan": "npm audit --audit-level=moderate && snyk test",
    "security:check": "semgrep --config=auto .",
    "security:pentest": "zap-baseline.py -t http://localhost:3000",
    "security:dast": "npm run security:pentest",
    "compliance:check": "node scripts/compliance-check.js",
    "architecture:validate": "node scripts/architecture-validate.js",
    "quality:gates": "node scripts/quality-gates.js",
    "rules:validate": "node scripts/rule-validation-windows.js",
    "rules:check": "npm run rules:validate",
    "rules:monitor": "node scripts/rule-validation-windows.js --watch",
    "deploy:staging": "node scripts/automated-deployment.js staging",
    "deploy:production": "node scripts/automated-deployment.js production",
    "monitor:start": "docker-compose -f monitoring/docker-compose.yml up -d",
    "monitor:stop": "docker-compose -f monitoring/docker-compose.yml down",
    "monitor:restart": "npm run monitor:stop && npm run monitor:start",
    "monitor:logs": "docker-compose -f monitoring/docker-compose.yml logs -f",
    "monitor:health": "bash scripts/health-check.sh",
    "monitor:metrics": "node scripts/collect-metrics.js",
    "monitor:setup": "node scripts/monitoring-setup.js",
    "health:check": "npm run monitor:health",
    "metrics:collect": "npm run monitor:metrics",
    "agent:start": "node scripts/agent-runner.js start",
    "agent:stop": "node scripts/agent-runner.js stop",
    "agent:status": "node scripts/agent-runner.js status",
    "agent:health": "node scripts/agent-runner.js health",
    "agent:task": "node scripts/agent-runner.js task",
    "agent:workflow": "node scripts/agent-runner.js workflow",
    "integration:test": "node scripts/test-cursor-n8n-integration.js",
    "integration:test:simple": "node scripts/test-integration-simple.js",
    "integration:status": "curl http://localhost:3001/api/integration/status",
    "browserbase:setup": "node scripts/setup-browserbase.js",
    "browserbase:test": "npm run test:browserbase",
    "browserbase:demo": "tsx src/apps/BrowserbaseDemo.ts",
    "docs:api:generate": "typedoc --out docs/api src",
    "docs:architecture:generate": "node scripts/generate-architecture-docs.js",
    "docs:readme:update": "node scripts/update-readme.js",
    "docs:deploy": "node scripts/deploy-docs.js",
    "changelog:generate": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "analyze:bundle": "npm run build && npx webpack-bundle-analyzer dist/assets/*.js",
    "analyze:quality": "sonar-scanner",
    "analyze:memory": "node --inspect scripts/memory-analysis.js",
    "complexity:analyze": "plato -r -d reports/plato src",
    "ai:suggest": "node scripts/ai-code-suggestions.js",
    "precommit": "npm run quality:gates",
    "premerge": "npm run test:unit:coverage && npm run test:integration && npm run security:scan",
    "predeploy": "npm run quality:gates && npm run test:e2e && npm run test:performance"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@modelcontextprotocol/sdk": "^1.18.0",
    "@openzeppelin/contracts": "^4.9.3",
    "@puppeteer/browsers": "^2.10.10",
    "@vitejs/plugin-react": "^4.0.3",
    "antd": "^5.27.4",
    "autoprefixer": "^10.4.14",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "chalk": "^5.6.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "hardhat": "^2.17.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "ora": "^9.0.0",
    "pg": "^8.11.3",
    "postcss": "^8.4.27",
    "prom-client": "^15.0.0",
    "prometheus-api-metrics": "^3.2.2",
    "puppeteer": "^21.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.7",
    "sharp": "^0.32.1",
    "socket.io": "^4.8.1",
    "stripe": "^14.7.0",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.0.0",
    "typescript": "^5.0.2",
    "uuid": "^9.0.0",
    "vite": "^4.4.5",
    "web3": "^4.1.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@playwright/test": "^1.36.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "artillery": "^2.0.0",
    "chai": "^6.0.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jsdom": "^27.0.0",
    "lighthouse": "^11.0.0",
    "lint-staged": "^13.2.3",
    "mocha": "^11.7.2",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run premerge"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/main.tsx",
      "!src/vite-env.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "**/__tests__/**/*.{ts,tsx}",
      "**/?(*.)+(spec|test).{ts,tsx}"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "yarn@4.9.4+sha512.7b1cb0b62abba6a537b3a2ce00811a843bea02bcf53138581a6ae5b1bf563f734872bd47de49ce32a9ca9dcaff995aa789577ffb16811da7c603dcf69e73750b"
}
