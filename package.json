    "@linear/sdk": "^60.0.0",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@svgdotjs/svg.js": "^3.2.4",
    "@tailwindcss/postcss": "^4.1.14",
    "d3": "^7.9.0",
    "mermaid": "^11.4.1",
    "react-svg": "^16.1.34",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "antd": "^5.27.4",
    "autoprefixer": "^10.4.21",
