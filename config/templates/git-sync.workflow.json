{
    "name": "git-sync-default",
    "description": "Synchronize DeepSeek remediation branches with environment-specific protections.",
    "branches": {
        "development": {
            "source": "main",
            "prefix": "fix/dev-deepseek-",
            "review": {
                "required": false,
                "labels": [
                    "automated",
                    "deepseek",
                    "dev"
                ]
            }
        },
        "staging": {
            "source": "develop",
            "prefix": "fix/stg-deepseek-",
            "review": {
                "required": true,
                "labels": [
                    "automated",
                    "deepseek",
                    "staging"
                ]
            }
        },
        "production": {
            "source": "release",
            "prefix": "fix/prod-deepseek-",
            "review": {
                "required": true,
                "labels": [
                    "automated",
                    "deepseek",
                    "prod"
                ],
                "approvers": [
                    "oncall-core"
                ]
            }
        }
    },
    "pipelines": {
        "preflight": [
            "npm run lint",
            "npm run test:unit"
        ],
        "postMerge": [
            "npm run compliance:check",
            "npm run automation:app-test"
        ]
    },
    "escalation": {
        "pagerduty": "pagerduty:core",
        "slackChannel": "#deepseek-remediation"
    }
}